

    <br />

    <h5>Aktif Sınavlar Sınavlar</h5>
    <br />
    <table class="table">
        <thead>
            <tr>
                <th scope="col">#</th>
                <th scope="col">Başlık</th>
                <th scope="col">Tarih</th>
                <th scope="col"></th>
            </tr>
        </thead>
        <tbody>
            @{int i = 1;}
            @foreach (var item in ViewBag.result)
            {
                <tr>
                    <th scope="row">@i</th>
                    <td>@item.Title</td>
                    <td>@item.Time</td>
                    <td> <a class="btn btn-info" asp-action="Show" asp-route-id="@item.ExamId" asp-controller="Home"> Sınava Gir </a></td>
                    @{i++;}
                </tr>
             }
           
        </tbody>
    </table>

    <hr />
